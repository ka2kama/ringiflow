  "query": "\n            SELECT\n                r.id,\n                r.tenant_id,\n                r.name,\n                r.description,\n                r.permissions,\n                r.is_system,\n                r.created_at,\n                r.updated_at\n            FROM roles r\n            INNER JOIN user_roles ur ON ur.role_id = r.id\n            WHERE ur.user_id = $1 AND ur.tenant_id = $2\n            ",
